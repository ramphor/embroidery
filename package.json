			"exports": "global:Backbone",
			"depends": [
				"jquery",
				"underscore"
			]
		},
		"marionette": {
			"exports": "global:Marionette",
			"depends": [
				"jquery",
				"underscore",
				"backbone"
			]
		}
	}
